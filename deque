#include <iostream>

 #include <deque>

 int main()

 {

 using namespace std;

 deque<int> deq;

 for (int nCount=0; nCount < 3; nCount++)

 { // insert at end of array

 deq.push_back(nCount);

 // insert at front of array

 deq.push_front(10 - nCount);

 }

 for (int nIndex=0; nIndex < deq.size(); nIndex++)

 cout << deq[nIndex] << " ";

 cout << endl;

 }
